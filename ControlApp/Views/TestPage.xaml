<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ClineControlApp.Views.TestPage"
             xmlns:viewmodels="clr-namespace:ClineControlApp.ViewModels"
             x:DataType="viewmodels:TestViewModel"
             >    
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Command="{Binding GoToLogin}" Text="Login"/>
            <Button Command="{Binding GoToDeviceDetail}" Text="DeviceDetail"/>
            <Button Command="{Binding GoToOverView}" Text="OverView"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>